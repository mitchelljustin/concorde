class Some
    def greet(input)
        if self.should_print
            print(self.name, input)
        end
    end

    def crash()
        print(self.non_existent_property)
    end
end
some = Some()
some.should_print = false
some.name = "Mitch"
some.greet("swag")
some.greet("swag")
some.should_print = true
some.greet("swag")
