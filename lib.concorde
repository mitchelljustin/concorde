class Range
    def init(start, finish)
        self.start = start
        self.finish = finish
    end

    def iter()
        self
    end

    def next()
        if self.start >= self.finish
            return nil
        end
        i = self.start
        self.start += 1
        i
    end
end

use IO
n = 0
s = ""
for i in Range(0, 10000)
    n += i
    s += "swag(" + i + ")"
    print(i, n, s)
end
