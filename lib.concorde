class Range
    def init(start, finish)
        self.start = start
        self.finish = finish
    end

    def iter()
        self
    end

    def next()
        if self.start >= self.finish
            return nil
        end
        i = self.start
        self.start += 1
        i
    end
end

range = Range(0, 100)
n = 0
for i in range
    n += i
    IO::print(i, n)
end
